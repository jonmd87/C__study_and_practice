"if something changed in ~/.vimrc 
"do this
" cp ~/.vimrc ~/programming


"set numbers
set number

"tab == 4 spaces
set tabstop=4

"highlightning for assambler syntax
set syn=nasm
"or
" set ft=nasm

"highlightning syntax
syntax on

"disable bold font (отменяет жирный шрифт)
set t_md=

"set dark backgrounde in vim
set background=dark

"change color of coments to [green]
hi comment ctermfg=darkgreen
